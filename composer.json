{
  "name": "t3kit/t3kit-starter",
  "description": "Starter point for t3kit project. t3kit and TYPO3 dependencies, folder structure, custom site configurations, local environment based on Docker, CI/CD configs",
  "type": "project",
  "keywords": [
    "t3kit",
    "typo3",
    "cms",
    "starter",
    "t3kit10"
  ],
  "homepage": "http://t3kit.com",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "t3kit Community",
      "homepage": "https://github.com/t3kit"
    }
  ],
  "support": {
    "issues": "https://github.com/t3kit/t3kit-starter/issues",
    "forum": "https://typo3.slack.com/messages/t3kit",
    "source": "https://github.com/t3kit/t3kit-starter",
    "docs": "https://github.com/t3kit/t3kit-starter"
  },
  "require": {
    "t3kit/t3kit": "dev-t3kit10",
    "typo3/cms-about": "^10.4.2",
    "typo3/cms-adminpanel": "^10.4.2",
    "typo3/cms-backend": "^10.4.2",
    "typo3/cms-belog": "^10.4.2",
    "typo3/cms-beuser": "^10.4.2",
    "typo3/cms-core": "^10.4.2",
    "typo3/cms-dashboard": "^10.4.2",
    "typo3/cms-extbase": "^10.4.2",
    "typo3/cms-extensionmanager": "^10.4.2",
    "typo3/cms-felogin": "^10.4.2",
    "typo3/cms-filelist": "^10.4.2",
    "typo3/cms-filemetadata": "^10.4.2",
    "typo3/cms-fluid": "^10.4.2",
    "typo3/cms-form": "^10.4.2",
    "typo3/cms-frontend": "^10.4.2",
    "typo3/cms-impexp": "^10.4.2",
    "typo3/cms-indexed-search": "^10.4.2",
    "typo3/cms-info": "^10.4.2",
    "typo3/cms-install": "^10.4.2",
    "typo3/cms-linkvalidator": "^10.4.2",
    "typo3/cms-lowlevel": "^10.4.2",
    "typo3/cms-opendocs": "^10.4.2",
    "typo3/cms-recordlist": "^10.4.2",
    "typo3/cms-recycler": "^10.4.2",
    "typo3/cms-redirects": "^10.4.2",
    "typo3/cms-reports": "^10.4.2",
    "typo3/cms-rte-ckeditor": "^10.4.2",
    "typo3/cms-scheduler": "^10.4.2",
    "typo3/cms-seo": "^10.4.2",
    "typo3/cms-setup": "^10.4.2",
    "typo3/cms-sys-note": "^10.4.2",
    "typo3/cms-t3editor": "^10.4.2",
    "typo3/cms-tstemplate": "^10.4.2",
    "typo3/cms-viewpage": "^10.4.2"
  },
  "require-dev": {
    "brainmaestro/composer-git-hooks": "^2.8",
    "heahdude/yaml-linter": "^1.0.3",
    "squizlabs/php_codesniffer": "^3.5.5"
  },
  "suggest": {},
  "autoload": {},
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "7.4"
    },
    "sort-packages": true
  },
  "scripts": {
    "typo3-cms-scripts": [
    ],
    "pre-install-cmd": [
      "composer validate --no-check-all --strict"
    ],
    "post-install-cmd": [
      "cghooks add --ignore-lock"
    ],
    "post-update-cmd": [
      "cghooks update"
    ],
    "post-autoload-dump": [
    ],
    "post-create-project-cmd": [
      "composer env"
    ],
    "env": "cp .localconf/docker/local.env .env",
    "dbup": "docker-compose exec -T web /var/www/html/.localconf/db/setupdb.sh",
    "dbre": "docker-compose exec -T web /var/www/html/.localconf/db/restoredb.sh",
    "dbp": "docker-compose exec -T web /var/www/html/.localconf/db/packdb.sh",
    "lint-php": "vendor/bin/phpcs --colors",
    "lint-yaml": "vendor/bin/yaml-linter --ansi config/ .localconf/",
    "test": [
      "@lint-php",
      "@lint-yaml"
    ],
    "ddev-config": "bash .localconf/community/ddev/setupddev.sh"
  },
  "extra": {
    "hooks": {
      "pre-commit": [
        "composer test"
      ]
    }
  }
}
